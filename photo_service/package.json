{
  "name": "photo_service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startdb": "mongod --dbpath ./database/data",
    "startdaemondb": "mongod --dbpath ./database/data --fork --logpath /dev/null",
    "stopdaemondb": "mongod --shutdown --dbpath ./database/data",
    "cleardb": "node ./database/clear.js",
    "seed": "node ./database/seed.js",
    "server": "nodemon ./server/index.js",
    "daemonserver": "forever start ./server/index.js",
    "cluster": "nodemon ./server/cluster.js",
    "server2": "nodemon ./server/index_server2.js",
    "server3": "nodemon ./server/index_server3.js",
    "server4": "nodemon ./server/index_server4.js",
    "startcache": "redis-server",
    "startdaemoncache": "redis-server --daemonize yes",
    "stopdaemoncache": "killall redis-server",
    "clearcache": "node ./cache/clear.js",
    "startlb": "sudo nginx",
    "stoplb": "sudo nginx -s stop"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cluster": "^0.7.7",
    "elastic-apm-node": "^0.9.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "mongoose": "^4.13.7",
    "multer": "^1.3.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "redis": "^2.8.0"
  }
}
